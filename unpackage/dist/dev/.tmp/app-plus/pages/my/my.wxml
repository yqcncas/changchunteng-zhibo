<view class="container"><view data-event-opts="{{[['tap',[['handleOpenDetail',['$event']]]]]}}" class="header right-arrow" bindtap="__e"><view class="user-info"><image class="avatar" src="{{userInfo.avatar}}" mode="aspectFill"></image><text>{{userInfo.userName}}</text></view></view><view class="collect-order"><view data-event-opts="{{[['tap',[['handleOpenOrder',['$event']]]]]}}" bindtap="__e"><image src="/static/icon/6.png"></image><text>我的订单</text></view><view data-event-opts="{{[['tap',[['handleOpenCollect',['$event']]]]]}}" bindtap="__e"><image src="../../static/icon/7.png"></image><text>我的收藏</text></view></view><view class="line-30"></view><view class="nav-list"><view data-event-opts="{{[['tap',[['handleOpenMyStudyMoney',['$event']]]]]}}" class="item right-arrow" bindtap="__e"><view class="money"><view><image src="/static/icon/8.png"></image><text>我的学币</text></view><view class="balance">{{"余额："+userInfo.allMoney}}</view></view></view><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['handleOpenNavDetail',['$0'],[[['navList','id',item.id]]]]]]]}}" class="item right-arrow" bindtap="__e"><view><image src="{{item.imgUrl}}"></image><text>{{item.name}}</text></view></view></block></view></view>