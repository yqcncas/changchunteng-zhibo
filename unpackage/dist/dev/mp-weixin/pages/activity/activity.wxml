<block wx:if="{{showContainer}}"><view class="container"><image class="header-img" src="{{detail.activity.pic}}" mode="aspectFill"></image><view class="active-title">打包课程</view><view class="course-list"><block wx:for="{{detail.courseList}}" wx:for-item="item" wx:for-index="__i0__"><view data-event-opts="{{[['tap',[['handleOpenCourseDetail',['$event']]]]]}}" class="course-item" bindtap="__e"><view class="course-name"><image class="course-name-bg" src="data:image/png;base64,"></image><text class="course-name-text">{{item.course.courseName}}</text></view><view class="price"><text class="price-text">{{"¥"+item.course.price}}</text></view><text class="course-title">{{item.course.name}}</text><text class="course-time">{{item.course.classTime}}</text><view class="course-level"><text class="course-level-title">难度</text><block wx:for="{{item.course.level}}" wx:for-item="star" wx:for-index="__i1__"><image class="course-level-star" src="data:image/png;base64," mode="aspectFill"></image></block></view><view class="teacher"><view class="teacher-left"><image class="teacher-avatar" src="{{baseURL+item.course.avatar}}" mode="aspectFill"></image><text class="teacher-name">{{item.course.teacherName}}</text></view><text class="course-collection">{{"共"+item.course.childNum+"讲"}}</text></view></view></block></view><view class="come-buy"><view><text>打包优惠价：</text><text>{{"¥"+detail.activity.price}}</text></view><view data-event-opts="{{[['tap',[['handleAwayBuy',['$event']]]]]}}" class="{{[detail.flag==-1?'no-buy':'']}}" bindtap="__e">立即支付</view></view></view></block>