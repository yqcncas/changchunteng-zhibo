/* 顶部阴影 */
page::after {
  content: "";
  position: fixed;
  top: 0;
  left: 0;
  box-shadow: inset 0 4rpx 8rpx 0 #f4f4f4;
  z-index: 9;
  width: 750rpx;
  height: 8rpx;
}
.container .qrcode-container {
  color: #FFFFFF;
  overflow: hidden;
  width: 570rpx;
  height: 714rpx;
  background: #8987DF;
  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.15), 0 6px 12px 0 rgba(0, 0, 0, 0.12);
  border-radius: 7px;
  margin: 0 auto;
  margin-top: 150rpx;
}
.container .qrcode-container view:first-child {
  text-align: center;
  margin-top: 60rpx;
  margin-bottom: 30rpx;
}
.container .qrcode-container .qrcode-content {
  width: 460rpx;
  height: 460rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.container .qrcode-container .qrcode-content image {
  width: 366rpx;
  height: 366rpx;
}
.container .qrcode-container .invitation-info {
  margin-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.container .qrcode-container .invitation-info text:last-child {
  margin-left: 30rpx;
  font-size: 20px;
}
.container .handle-container {
  width: 100vw;
  height: 90rpx;
  position: fixed;
  left: 0;
  bottom: 30rpx;
}
.container .handle-container > view {
  position: absolute;
  top: 0;
  right: 30rpx;
  width: 690rpx;
  height: 90rpx;
  background: #8987DF;
  border-radius: 4px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #FFFFFF;
}
.container .handle-container > view:active {
  opacity: 0.8;
}

