<view class="container"><view class="header"><image class="teacherPic" src="{{teacherDetail.pic}}" mode="aspectFill"></image><view class="info"><view class="name">{{teacherDetail.userName}}</view><view class="type">{{teacherDetail.lessons}}</view><view class="labels"><text>{{teacherDetail.edu}}</text><text>{{"教龄"+teacherDetail.years+"年"}}</text></view></view></view><view class="teacher-tabs"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{[!tabSelect?'select':'']}}" bindtap="__e">介绍</view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{[tabSelect?'select':'']}}" bindtap="__e">课程</view></view><view class="content"><view hidden="{{!(tabSelect===false)}}" style="padding:30rpx;"><parser vue-id="5a3b8480-1" html="{{teacherDetail.remark}}" show-with-animation lazy-load bind:__l="__l" vue-slots="{{['default']}}">加载中...</parser></view><view hidden="{{!(tabSelect===true)}}" class="course-list"><block wx:for="{{courseList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="courseId"><view data-event-opts="{{[['tap',[['handleOpenCourseDetail',['$0'],[[['courseList','courseId',item.courseId,'courseId']]]]]]]}}" class="course-item" bindtap="__e"><view class="course-name"><image class="course-name-bg" src="data:image/png;base64,"></image><text class="course-name-text">{{item.courseName}}</text></view><view class="price"><text class="price-text">{{"¥"+item.price}}</text></view><text class="course-title">{{item.name}}</text><text class="course-time">{{item.classTime}}</text><view class="course-level"><text class="course-level-title">难度</text><block wx:for="{{item.level}}" wx:for-item="starItem" wx:for-index="__i1__"><image class="course-level-star" src="data:image/png;base64," mode="aspectFill"></image></block></view><view class="teacher"><view class="teacher-left"><image class="teacher-avatar" src="{{''+baseURL+item.avatar}}" mode="aspectFill"></image><text class="teacher-name">{{item.teacherName}}</text></view><text class="course-collection">{{"共"+item.childNum+"讲"}}</text></view></view></block></view></view></view>